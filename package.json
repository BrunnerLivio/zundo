{
  "name": "zundo",
  "version": "0.1.0",
  "private": false,
  "description": "🍜 Undo middleware for your favorite, comfy state-management solution: [zustand](https://github.com/pmndrs/zustand).",
  "keywords": [
    "undo",
    "middleware",
    "zustand"
  ],
  "license": "MIT",
  "author": "Charles Kornoelje",
  "main": "dist/index.js",
  "module": "dist/zundo.esm.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "analyze": "size-limit --why",
    "build": "tsdx build",
    "build-storybook": "build-storybook",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "start": "tsdx watch",
    "storybook": "start-storybook -p 6006",
    "test": "tsdx test --passWithNoTests"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "dependencies": {
    "zustand": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.13.14",
    "@size-limit/preset-small-lib": "^4.10.2",
    "@storybook/addon-essentials": "^6.2.1",
    "@storybook/addon-info": "^5.3.21",
    "@storybook/addon-links": "^6.2.1",
    "@storybook/addons": "^6.2.1",
    "@storybook/react": "^6.2.1",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "babel-loader": "^8.2.2",
    "husky": "^6.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "size-limit": "^4.10.2",
    "tsdx": "^0.14.1",
    "tslib": "^2.1.0",
    "typescript": "^4.2.3"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "engines": {
    "node": ">=10"
  },
  "size-limit": [
    {
      "path": "dist/zundo.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/zundo.esm.js",
      "limit": "10 KB"
    }
  ]
}
